{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Generators": {
    "Generator1": {
      "GenerationPeriodSeconds": "00:00:05",
      "TaskId": 1,
      "Destination": "Main"
    },
    "Generator2": {
      "GenerationPeriodSeconds": "00:00:15",
      "TaskId": 2,
      "Destination": "Main"
    }
  },
  "MessageMemoryChannelConfiguration": {
    "Capacity": 100
  },
  "DestinationSenderConfiguration": {
    "SendHighWatermark": 100,
    "Address": "tcp://*:12345"
  },
  "AllowedHosts": "*"
}
